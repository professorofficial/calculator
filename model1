
public class Calculator {
	public static void  main(String[]args)
	{
		System.out.println("Calculator");
		Scanner sc = new Scanner (System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = a+b;
		int d = a-b;
		int e = a*b;
		int f = a/b;
		int g = a^b;
		String operation = sc.nextLine();
		if(operation == "Addition")
		{
			System.out.println(c);
		}
	}
	

}
